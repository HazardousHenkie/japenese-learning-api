{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": false,
        "baseUrl": "src",
        "noEmit": true,
        "moduleResolution": "node",
        "pretty": true
    },
    "exclude": ["node_modules"]
}
